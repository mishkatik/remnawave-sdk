  },
  "keywords": [
    "remnawave",
    "sdk",
    "api"
  ],
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@types/bun": "1.3.6"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@remnawave/backend-contract": "2.5.13",
    "axios": "^1.13.2",
    "zod": "3.25.76"
  }
}
